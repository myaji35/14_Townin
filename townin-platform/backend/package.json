{
  "name": "townin-backend",
  "version": "1.0.0",
  "description": "Townin Platform Backend API (NestJS)",
  "author": "Townin Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "typeorm migration:generate -d src/data-source.ts",
    "migration:run": "typeorm migration:run -d src/data-source.ts",
    "migration:revert": "typeorm migration:revert -d src/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts all",
    "seed:regions": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts regions",
    "seed:grid-cells": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts grid-cells",
    "seed:clear": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts clear",
    "seed:stats": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts stats",
    "seed:test": "ts-node scripts/seed-test-data.ts",
    "test:api": "./scripts/test-e2e.sh",
    "test:integration": "./scripts/test-integration.sh",
    "setup:test": "./scripts/setup-test-env.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@google-cloud/vision": "^4.2.0",
    "@influxdata/influxdb-client": "^1.33.2",
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.4.20",
    "@sentry/node": "^10.27.0",
    "@sentry/profiling-node": "^10.27.0",
    "@types/cors": "^2.8.19",
    "@types/geojson": "^7946.0.16",
    "@types/passport-google-oauth20": "^2.0.17",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "h3-js": "^4.3.0",
    "langchain": "^0.1.9",
    "multer": "^2.0.2",
    "neo4j-driver": "^5.16.0",
    "nest-winston": "^1.10.2",
    "openai": "^6.9.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-naver-v2": "^2.0.8",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.19",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
